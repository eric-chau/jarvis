{
    "name": "eric-chau/jarvis",
    "description": "Jarvis is a PHP 7 micro-framework",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "nikic/fast-route": "0.*",
        "symfony/http-foundation": "~3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.1.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Jarvis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jarvis\\Tests\\": "tests/"
        }
    }
}
