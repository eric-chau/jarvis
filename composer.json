{
    "name": "eric-chau/jarvis",
    "description": "Jarvis is a PHP 7 micro-framework",
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "nikic/fast-route": "~1.2.0",
        "symfony/http-foundation": "~3.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "codeclimate/php-test-reporter": "~0.4.0"
    },
    "autoload": {
        "psr-4": {
            "Jarvis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jarvis\\Tests\\": "tests/"
        }
    }
}
